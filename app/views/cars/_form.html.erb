<%= simple_form_for(car) do |f| %>
  <%= f.input :name %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.input :location %>
  <%= f.input :description %>
  <%= f.input :capacity %>
  <%= f.input :price %>
  <%= f.input :min_age %>
  <%= f.input :make %>
  <%= f.input :model %>
  <%= f.input :ac %>
  <%= f.input :fuel %>
  <%= f.input :consumption %>
  <%= f.input :kilometers %>
  <%= f.input :year %>


  <%= f.submit %>


<% end %>
