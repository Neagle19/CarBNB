<h1>Cars</h1>

<% @cars.each do |car| %>
<div class="class-card ">
  <%= image_tag('https://picsum.photos/200/300') %>
  <%= link_to car_path(car.id) do %>
    <%= car.name %><br>
    <%= car.description %><br>
    <%= car.location %><br>
    <%= car.price %><br>
    <br>
   </div>

  <% end %>
<% end %>
<img src="https://picsum.photos/200/300" alt="">
