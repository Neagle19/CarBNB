<h1>Cars</h1>

<%= link_to "Click here to see our top rated cars", car_top_path(@cars) %>

<% @cars.each do |car| %>
<div class="class-card ">
  <%= cl_image_tag(car.photo) %>
  <%= link_to car_path(car.id) do %>
    <%= car.name %><br>
    <%= car.description %><br>
    <%= car.location %><br>
    <%= car.price %><br>
    <br>
   </div>

  <% end %>
<% end %>
<img src="https://picsum.photos/200/300" alt="">
